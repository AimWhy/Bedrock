{
  "extends": [
    "react-app",
    "plugin:jsx-a11y/recommended",
    "eslint-config-prettier",
    "plugin:storybook/recommended"
  ],
  "plugins": ["jsx-a11y"],
  "ignorePatterns": ["node_modules", "dist"],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": 0,
    "no-console": "error"
  }
}
